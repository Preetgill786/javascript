    <script>
        // write some JS code to add numbers 1000 down to 100 step down by 17   
     // add up each number you step over  
      // display the result
function a(){
  var sum =0;
  startNumber = 1000;
  endNumber = 100;
  stepNumber = -17;
  currentNumber = startNumber;
    for(;currentNumber>100;){
        
        sum = sum + currentNumber;
        currentNumber += stepNumber;
        
         
    



}
return sum;
}

document.write("sum of numbers from 1000 to 100:",a());
// using while loop

sum = 0;
   i=1000;
   while(i> 100){
    sum += i;
    i -= 17
   }

   alert("The sum is:" +sum);
    </script>
